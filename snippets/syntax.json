    },
    "with multiline": {
        "prefix": "withm",
        "body": [
            "with (",
            "\t${1:context1} as ${2:alias1}",
            "\t${3:context2} as ${4:alias2}",
            "):",
            "\t${0:pass}"
        ],
        "description": "multi-line with satement"
    },
    "match": {
        "prefix": "match",
        "body": [
            "match ${1:expression}:",
            "\tcase ${2:patterns}:",
            "\t\t${0:pass}"
        ],
        "description": "match case block"
    },
    "case": {
        "prefix": "case",
        "body": [
