    "try/except/else/finally": {
        "prefix": "tryef",
        "body": [
            "try:",
            "\t${1:pass}",
            "except ${2:Exception} as ${3:err}:",
            "\t${4:pass}",
            "else:",
            "\t${5:pass}",
