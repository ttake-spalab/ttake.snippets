        "description": "`yield from` statement"
    },
    "raise": {
        "prefix": "ra",
        "body": "raise ${0:Exception}",
        "description": "`raise` statement"
    },
    "raise from": {
        "prefix": ["raf", "raisef"],
        "body": "raise ${1:Exception} from ${0:err}",
        "description": "`raise` statement"
    },
    "import": {
        "prefix": "im",
        "body": "import ${0:package/module}",
        "description": "`import` statement"
    },
    "import as": {
        "prefix": "imas",
        "body": "import ${1:package/module} as ${0:alias}",
