    },
    "type alias": {
        "prefix": ["tya", "typealias"],
        "body": "type ${1:Alias} = ${0:expression}",
        "description": "type alise statement"
    },
    "lambda": {
        "prefix": "lambda",
        "body": "lambda ${1:args}: ${0:expression}",
        "description": "`lambda` statement"
    }
