    "except": {
        "prefix": "except",
        "body": [
            "except ${1:Exception} as ${2:err}:",
            "\t${0:pass}"
        ],
        "description": "exception statement"
    },
    "except-star": {
        "prefix": "excepts",
        "body": [
            "except* ${1:Exception} as ${2:err}:",
            "\t${0:pass}"
        ],
        "description": "exception* statement"
    },
    "with": {
        "prefix": "with",
        "body": [
            "with ${1:context}:",
            "\t${0:pass}"
        ],
        "description": "with statement"
    },
