    },
    "function define": {
        "prefix": "def",
        "body": [
            "def ${1:fname}(${2:arg: types})$3:",
            "\t${0:pass}"
        ],
        "description": "function definitions"
    },
    "function w/ typehint": {
        "prefix": ["defty", "defhint"],
        "body": [
            "def ${1:fname}(${2:args: types}) -> ${3:types}:",
            "\t${0:pass}"
        ],
        "description": "function definitions with type hint"
    },
    "class": {
        "prefix": "class",
        "body": [
            "class ${1:ClassName}:",
            "\t${0:pass}"
        ],
        "description": "class definitions"
    },
    "class": {
        "prefix": "classi",
        "body": [
            "class ${1:ClassName}:",
            "\t\"\"\"${3:docstring for ${1:ClassName}}\"\"\"",
            "\tdef __init__(self, ${args: types}) -> None:",
            "\t\tsuper(${1:ClassName}, self).__init__()",
            "\t\t$0"
        ],
        "description": "class definitions template"
    },
    "method": {
        "prefix": "defs:",
        "body": [
            "def ${1:fname}(self, ${2:args: types}) -> ${3:types}:",
            "\t${0:pass}"
        ],
        "description": "method definition"
    },
    "class method": {
        "prefix": "defc",
        "body": [
            "@classmethod",
            "def ${1:fname}(cls, ${2:args: types}) -> ${3:types}:",
            "\t${0:pass}"
        ],
        "description": "class method defintion"
    },
    "static method": {
        "prefix": "defst",
        "body": [
            "@staticmethod",
            "def ${1:fname}(${2:args: types}) -> ${3:types}:",
            "\t${0:pass}"
        ],
        "description": "static method definition"
    },
    "dunder method": {
        "pefix": "defd",
        "body": [
            "def __${1:fname}__(self, ${2:args: types}) -> ${3:types}",
            "\t${0:pass}"
        ],
        "description": "special method definition"
    },
    "property getter": {
        "prefix": "@property",
        "body": [
            "@property",
            "def ${1:var}(self) -> ${2:types}:",
            "\treturn ${0:self._${1:var}}"
        ],
        "description": "property getter definition"
    },
    "property setter": {
        "prefix": "@property.setter",
        "body": [
            "@${1:var}.setter",
            "def ${1:var}(self, ${2:value}):",
            "\t${0:self._${1:var}} = ${2:value}"
        ],
        "description": "property setter definition"
    },
    "async function": {
        "prefix": "adef",
        "body": [
            "async def ${1:fname}(${2:args: types}) -> ${3:types}:",
            "\t${0:pass}"
        ],
        "description": "coroutine function definition"
    },
    "async for": {
        "prefix": "afor",
        "body": [
            "async for ${1:value} in ${2:iterable}:",
