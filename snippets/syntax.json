{
    "if": {
        "prefix": "if",
        "body": [
            "if ${1:condition}:",
            "\t${0:pass}"
        ],
        "description": "`if` statement"
    },
    "else if": {
        "prefix": "elif",
        "body": [
            "elif ${1:condition}:",
            "\t${0:pass}"
        ],
        "description": "else if statement"
    },
    "else": {
        "prefix": "else",
        "body": [
            "else ${1:condition}:",
            "\t${0:pass}"
        ],
        "description": "else statement"
    },
    "if/else": {
        "prefix": "ife",
        "body": [
            "if ${1:condition}:",
            "\t${2:pass}",
            "else:",
            "\t${0:pass}"
        ],
        "description": "if else statement"
    },
    "if/elif/else": {
        "prefix": "ifee",
        "body": [
            "if ${1:condition}:",
            "\t${2:pass}",
            "elif ${3:condition}:",
            "\t${4:pass}",
            "else:",
            "\t${0:pass}"
        ],
        "description": "if elif else statement"
    },
